    "@types/node": "10.12.2",
    "@types/winston": "2.3.7",
    "jest": "23.4.2",
    "jest-preset-typescript": "1.0.1",
    "nodemon": "1.18.5",
    "rimraf": "2.6.2",
    "ts-jest": "23.1.4",
    "ts-node": "7.0.1",
    "typescript": "3.1.1"
